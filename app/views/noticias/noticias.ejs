<!DOCTYPE html>
<html lang="pt-br">
	<meta charset="utf-8"/>
	<body>
		<h1>News </h1>

		<table>
			<thead>
				<tr>
					<th>Titulo</th>
				</tr>
			</thead>
			<tbody>
				<% noticias.forEach( noticia => { %>
					<tr>
						<td><a href="/noticia/<%= noticia.id %>"><%= noticia.titulo %></a></td>
					</tr>
				<% }); %>
			</tbody>
		</table>
	</body>
</html>